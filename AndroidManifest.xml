<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tekmob.nfc.note_u_list"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="tekmob.nfc.note_u_list.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.CameraActivity"
            android:label="@string/title_activity_camera" >
        </activity>
         <activity
            android:name="tekmob.nfc.note_u_list.AudioRecordTest"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.ViewNoteActivity"
            android:label="@string/title_activity_view_note" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.NoteActivity"
            android:label="@string/title_activity_note" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.AudioRecordingActivity"
            android:label="@string/title_activity_voice_rec" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.ShareActivity"
            android:label="@string/title_activity_share" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.SplashActivity"
            android:label="@string/title_activity_splash" >
        </activity>
        <activity
            android:name="tekmob.nfc.note_u_list.ManActivity"
            android:label="@string/title_activity_man" >
        </activity>
    </application>

</manifest>
